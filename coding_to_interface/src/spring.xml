<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
 <bean id="triangle" class= "coding_to_interface.Triangle">
 <property name= "pointA"   ref= "pointA"/>
 <property name= "pointB"   ref= "pointB"/>
 <property name= "pointC"   ref= "pointC"/>
 
 
 
 </bean>
 
 <bean id="circle"  class= "coding_to_interface.Circle">
   <property name="center" ref= "pointA"/>
 
 </bean>
 
 <bean id="pointA"   class= "coding_to_interface.Point" >
 <property name= "x"  value= "0"></property>
 <property name= "Y"  value= "0"></property>
 
 </bean>


 <bean id="pointB"   class= "coding_to_interface.Point" >
 <property name= "x"  value= "20"></property>
 <property name= "Y"  value= "0"></property>
 
 </bean>

 <bean id="pointC"   class= "coding_to_interface.Point" >
 <property name= "x"  value= "0"></property>
 <property name= "Y"  value= "30"></property>
 
 </bean>


</beans>