<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
<bean id= "triangle"  class= "intialise_List_datamember.Triangle">
<property name="points">
<list>
   <ref bean="getpoint0" /> 
   <ref bean="getpoint1" /> 
   <ref bean="getpoint2" /> 
   
   

</list>


</property>



</bean>

<bean id= "getpoint0"  class="intialise_List_datamember.Point">
<property  name="X" value= "0"></property>
<property  name="Y" value= "0"></property>


</bean>



    
  <bean id= "getpoint1" class="intialise_List_datamember.Point">
     <property  name="X" value= "20"></property>
     <property  name="Y" value= "0"></property>

   </bean>
    
   <bean  id="getpoint2" class="intialise_List_datamember.Point">
     <property  name="X" value= "0"></property>
      <property  name="Y" value= "20"></property>

  </bean>


</beans>